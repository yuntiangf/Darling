<?xml version="1.0" encoding="utf-8"?>
<!--我穿我搭 -->
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:orientation="vertical" >
    
	<LinearLayout
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:orientation="vertical">
    	<ImageView
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:background="@drawable/divider_horizontal"
		    ></ImageView>
		<LinearLayout
	         android:layout_width="match_parent"
	         android:layout_height="50dp"
	         android:orientation="horizontal" >
	         <ImageView
	             android:layout_width="wrap_content"
	             android:layout_height="15dp"
	             android:layout_gravity="center_vertical|left"
	             android:layout_marginLeft="10dp"
	             android:scaleType="fitStart"
	             android:src="@drawable/text3" />
	     </LinearLayout>
		<ImageView
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:background="@drawable/divider_horizontal"
		    ></ImageView>
		<LinearLayout
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:orientation="horizontal">

	         <LinearLayout
	             android:id="@+id/three_minutes"
	             android:layout_width="0dp"
	             android:layout_height="wrap_content"
	             android:gravity="center"
	             android:layout_weight="1"
	             android:clickable="true"
	             android:orientation="vertical" >

	             <ImageView
	                 android:layout_width="65dp"
	                 android:layout_height="65dp"
	                 android:layout_marginTop="20dp"
	                 android:src="@drawable/wd_threeminutes" />

	             <TextView
	                 android:layout_width="wrap_content"
	                 android:layout_height="wrap_content"
	                 android:layout_marginBottom="10dp"
	                 android:text="三分钟出门穿什么？"	                 
	                 android:textColor="@color/dimgray" />
	         </LinearLayout>
		    
		    <ImageView 
		        android:layout_width="wrap_content"
		        android:layout_height="match_parent"
		        android:background="@drawable/divider_vertical"/>
		     <LinearLayout
	             android:id="@+id/one_minute"
	             android:layout_width="0dp"
	             android:layout_height="wrap_content"
	             android:gravity="center"
	             android:layout_weight="1"
	             android:clickable="true"
	             android:orientation="vertical" >

	             <ImageView
	                 android:layout_width="65dp"
	                 android:layout_height="65dp"
	                 android:layout_marginTop="20dp"
	                 android:src="@drawable/wd_oneminute" />

	             <TextView
	                 android:layout_width="wrap_content"
	                 android:layout_height="wrap_content"
	                 android:layout_marginBottom="10dp"
	                 android:text="一分钟出门摇一摇"	                 
	                 android:textColor="@color/dimgray" />
	         </LinearLayout>
		</LinearLayout>
		<LinearLayout
		    android:layout_height="wrap_content"
		    android:layout_width="match_parent"
		    android:orientation="horizontal"
		    android:gravity="center">
		    <ImageView
		        android:layout_width="0dp"
		        android:layout_height="wrap_content"
		        android:layout_weight="1"
		        android:background="@drawable/divider_horizontal"/>

		    <ImageView
		        android:layout_width="10dp"
		        android:layout_height="10dp"
		        android:layout_margin="2dp"
		        android:src="@drawable/corner" />
			<ImageView
		        android:layout_width="0dp"
		        android:layout_height="wrap_content"
		        android:layout_weight="1"
		        android:background="@drawable/divider_horizontal"/>
		</LinearLayout>				
		<LinearLayout
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:orientation="horizontal">
		    <LinearLayout
	             android:id="@+id/wd_personal"
	             android:layout_width="0dp"
	             android:layout_height="wrap_content"
	             android:gravity="center"
	             android:layout_weight="1"
	             android:clickable="true"
	             android:orientation="vertical" >

	             <ImageView
	                 android:layout_width="65dp"
	                 android:layout_height="65dp"
	                 android:layout_marginTop="20dp"
	                 android:src="@drawable/wd_personal" />

	             <TextView
	                 android:layout_width="wrap_content"
	                 android:layout_height="wrap_content"
	                 android:layout_marginBottom="10dp"
	                 android:text="个人特质分析"	                 
	                 android:textColor="@color/dimgray" />
	         </LinearLayout>
		    
		    <ImageView 
		        android:layout_width="wrap_content"
		        android:layout_height="match_parent"
		        android:background="@drawable/divider_vertical"/>
		     <LinearLayout
	             android:id="@+id/wd_recommend"
	             android:layout_width="0dp"
	             android:layout_height="wrap_content"
	             android:gravity="center"
	             android:layout_weight="1"
	             android:clickable="true"
	             android:orientation="vertical" >

	             <ImageView
	                 android:layout_width="65dp"
	                 android:layout_height="65dp"
	                 android:layout_marginTop="20dp"
	                 android:src="@drawable/wd_recommend" />

	             <TextView
	                 android:layout_width="wrap_content"
	                 android:layout_height="wrap_content"
	                 android:layout_marginBottom="10dp"
	                 android:text="穿搭整体搭配建议"	                 
	                 android:textColor="@color/dimgray" />
	         </LinearLayout>
		</LinearLayout>
		<LinearLayout
		    android:layout_height="wrap_content"
		    android:layout_width="match_parent"
		    android:orientation="horizontal"
		    android:gravity="center">
		    <ImageView
		        android:layout_width="0dp"
		        android:layout_height="wrap_content"
		        android:layout_weight="1"
		        android:background="@drawable/divider_horizontal"/>

		    <ImageView
		        android:layout_width="10dp"
		        android:layout_height="10dp"
		        android:layout_margin="2dp"
		        android:src="@drawable/corner" />
			<ImageView
		        android:layout_width="0dp"
		        android:layout_height="wrap_content"
		        android:layout_weight="1"
		        android:background="@drawable/divider_horizontal"/>
		</LinearLayout>			
		<LinearLayout
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:orientation="horizontal">
		    <LinearLayout
	             android:id="@+id/wd_myitems"
	             android:layout_width="0dp"
	             android:layout_height="wrap_content"
	             android:gravity="center"
	             android:layout_weight="1"
	             android:clickable="true"
	             android:orientation="vertical" >

	             <ImageView
	                 android:layout_width="65dp"
	                 android:layout_height="65dp"
	                 android:layout_marginTop="20dp"
	                 android:src="@drawable/wd_myitems" />

	             <TextView
	                 android:layout_width="wrap_content"
	                 android:layout_height="wrap_content"
	                 android:layout_marginBottom="10dp"
	                 android:text="个人单品分析"	                 
	                 android:textColor="@color/dimgray" />
	         </LinearLayout>
		    
		    <ImageView 
		        android:layout_width="wrap_content"
		        android:layout_height="match_parent"
		        android:background="@drawable/divider_vertical"/>
		     <LinearLayout
	             android:id="@+id/wd_myalmirah"
	             android:layout_width="0dp"
	             android:layout_height="wrap_content"
	             android:gravity="center"
	             android:layout_weight="1"
	             android:clickable="true"
	             android:orientation="vertical" >

	             <ImageView
	                 android:layout_width="65dp"
	                 android:layout_height="65dp"
	                 android:layout_marginTop="20dp"
	                 android:src="@drawable/wd_myalmirah" />

	             <TextView
	                 android:layout_width="wrap_content"
	                 android:layout_height="wrap_content"
	                 android:layout_marginBottom="10dp"
	                 android:text="个人衣橱分析"	                 
	                 android:textColor="@color/dimgray" />
	         </LinearLayout> 
		</LinearLayout>	
		<LinearLayout
		    android:layout_height="wrap_content"
		    android:layout_width="match_parent"
		    android:orientation="horizontal"
		    android:gravity="center">
		    <ImageView
		        android:layout_width="0dp"
		        android:layout_height="wrap_content"
		        android:layout_weight="1"
		        android:background="@drawable/divider_horizontal"/>

		    <ImageView
		        android:layout_width="10dp"
		        android:layout_height="10dp"
		        android:layout_margin="2dp"
		        android:src="@drawable/corner" />
			<ImageView
		        android:layout_width="0dp"
		        android:layout_height="wrap_content"
		        android:layout_weight="1"
		        android:background="@drawable/divider_horizontal"/>
		</LinearLayout>	
		<LinearLayout
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:orientation="horizontal">
		    <LinearLayout
	             android:id="@+id/wd_advise"
	             android:layout_width="0dp"
	             android:layout_height="wrap_content"
	             android:gravity="center"
	             android:layout_weight="1"
	             android:clickable="true"
	             android:orientation="vertical" >

	             <ImageView
	                 android:layout_width="65dp"
	                 android:layout_height="65dp"
	                 android:layout_marginTop="20dp"
	                 android:src="@drawable/wd_advise" />

	             <TextView
	                 android:layout_width="wrap_content"
	                 android:layout_height="wrap_content"
	                 android:layout_marginBottom="10dp"
	                 android:text="专属推荐入手"	                 
	                 android:textColor="@color/dimgray" />
	         </LinearLayout>
		    
		    <ImageView 
		        android:layout_width="wrap_content"
		        android:layout_height="match_parent"
		        android:background="@drawable/divider_vertical"/>
		     <LinearLayout
	             android:id="@+id/wd_goods"
	             android:layout_width="0dp"
	             android:layout_height="wrap_content"
	             android:gravity="center"
	             android:layout_weight="1"
	             android:clickable="true"
	             android:orientation="vertical" >

	             <ImageView
	                 android:layout_width="65dp"
	                 android:layout_height="65dp"
	                 android:layout_marginTop="20dp"
	                 android:src="@drawable/wd_goods" />

	             <TextView
	                 android:layout_width="wrap_content"
	                 android:layout_height="wrap_content"
	                 android:layout_marginBottom="10dp"
	                 android:text="好货发现"	                 
	                 android:textColor="@color/dimgray" />
	         </LinearLayout> 
		</LinearLayout>	
		<ImageView
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:background="@drawable/divider_horizontal"
		    ></ImageView>
		<Space
		    android:layout_height="5dp"
		    android:layout_width="match_parent"></Space>	
		<ImageView
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:background="@drawable/divider_horizontal"
		    ></ImageView>
	</LinearLayout>
</ScrollView>
