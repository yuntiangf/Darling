package com.darling.model;

public class RecommendInfo {
	
		//推荐识别码id
		private int rec_id;
		
		//推荐模特image
		private int rec_image;
		
		//推荐单品1
		private int rec_item1;
		
		//推荐单品2
		private int rec_item2;
				
		//喜欢人数
		private int lover_num;
		
		//商品品牌
		private String rec_brand;
		
		//商品价格
		private String rec_price;
				
		//推荐商品风格
		private String rec_style;
		
		//推荐商品描述
		private String rec_description;

		public RecommendInfo() {
			
		}
		//“发现”的推荐界面用到的构造函数
		public RecommendInfo(int rec_image, int rec_item1, int rec_item2) {
			super();
			this.rec_image = rec_image;
			this.rec_item1 = rec_item1;
			this.rec_item2 = rec_item2;
		}

		public int getRec_id() {
			return rec_id;
		}

		public void setRec_id(int rec_id) {
			this.rec_id = rec_id;
		}

		public int getRec_image() {
			return rec_image;
		}

		public void setRec_image(int rec_image) {
			this.rec_image = rec_image;
		}

		public int getRec_item1() {
			return rec_item1;
		}

		public void setRec_item1(int rec_item1) {
			this.rec_item1 = rec_item1;
		}

		public int getRec_item2() {
			return rec_item2;
		}

		public void setRec_item2(int rec_item2) {
			this.rec_item2 = rec_item2;
		}

		public int getLover_num() {
			return lover_num;
		}

		public void setLover_num(int lover_num) {
			this.lover_num = lover_num;
		}

		public String getRec_brand() {
			return rec_brand;
		}

		public void setRec_brand(String rec_brand) {
			this.rec_brand = rec_brand;
		}

		public String getRec_price() {
			return rec_price;
		}

		public void setRec_price(String rec_price) {
			this.rec_price = rec_price;
		}

		public String getRec_style() {
			return rec_style;
		}

		public void setRec_style(String rec_style) {
			this.rec_style = rec_style;
		}

		public String getRec_description() {
			return rec_description;
		}

		public void setRec_description(String rec_description) {
			this.rec_description = rec_description;
		}

		
		

		
}
